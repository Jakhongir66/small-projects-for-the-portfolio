    "express": "^4.18.2",
    "jquery": "^3.6.3",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.8.4",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pinia": "^2.0.28",
    "swiper": "^8.4.6",
    "vue": "^3.2.45",
