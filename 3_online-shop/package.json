    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "ts-node": "^10.9.1",
